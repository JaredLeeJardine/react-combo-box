{
  "name": "react-combo-box",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "playwright": "pnpm exec playwright test",
    "playwright:ui": "pnpm exec playwright test --ui",
    "test": "pnpm exec start-server-and-test dev http://localhost:3000 playwright",
    "test:ui": "pnpm exec start-server-and-test dev http://localhost:3000 playwright:ui"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@playwright/test": "1.37.1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "start-server-and-test": "2.0.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
